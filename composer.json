{
  "name": "diegofleitas/contentawarebot",
  "description": "Content Aware Facebook Bot",
  "authors": [
    {
      "name": "Diego Fleitas"
    }
  ],
  "require": {
    "php": "^5.4|^7.0",
    "intervention/image": "2.4.2",
    "ext-curl": "*",
    "danielstjules/stringy": "3.1.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "squizlabs/php_codesniffer": "3.4.0",
    "facebook/graph-sdk": "5.7.0",
    "giphy/giphy-php-client": "1.0.0",
    "php-ffmpeg/php-ffmpeg": "0.9.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.8",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "ContentAwareBot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ContentAwareBot\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "paragonie/random_compat": "Provides a better CSPRNG option in PHP 5",
    "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
  }
}
